
@{
    ViewBag.Title = "Details";
}

<h2>Department</h2>

<h3>@Model.Name</h3>

<h2>Students</h2>
<table class="table table-bordered text-center">
    <tr class="bg-info">
        <td>Id</td>
        <td>Name</td>
        <td>Dob</td>
        <td>Gender</td>
        <td>Cgpa</td>
    </tr>

    @foreach (var item in Model.Students)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Dob</td>
            <td>@item.Gender</td>
            <td>@item.Cgpa</td>
        </tr>
    }
</table>